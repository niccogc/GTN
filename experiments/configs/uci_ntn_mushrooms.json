{
  "experiment_name": "ntn_mushrooms",
  "dataset": "mushrooms",
  "task": "classification",
  "parameter_grid": {
    "model": [
      "MPO2",
      "MPO2TypeI"
    ],
    "L": [
      3,
      4
    ],
    "bond_dim": [
      8,
      12
    ],
    "jitter_start": [
      5.0
    ]
  },
  "fixed_params": {
    "batch_size": 512,
    "n_epochs": 100,
    "jitter_decay": 0.25,
    "adaptive_jitter": false,
    "patience": 10,
    "min_delta": 1e-05,
    "train_selection": true,
    "init_strength": 0.01,
    "seeds": [
      42,
      7,
      123,
      256,
      999
    ]
  },
  "tracker": {
    "backend": "both",
    "tracker_dir": "experiment_logs",
    "aim_repo": "aim://aimtracking.kosmon.org:443"
  },
  "output": {
    "results_dir": "results/ntn_mushrooms",
    "save_models": false,
    "save_individual_runs": true
  }
}